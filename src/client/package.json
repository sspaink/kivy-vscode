{
	"name": "kivy-vscode",
	"displayName": "Kivy",
	"description": "Kivy language support for Visual Studio Code.",
	"author": "Microsoft Corporation",
	"license": "MIT",
	"version": "0.1.0",
	"publisher": "vscode",
	"repository": {
		"type": "git",
		"url": "https://github.com/BattleBas/kivy-vscode"
	},
	"engines": {
		"vscode": "^1.25.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:kivy"
	],
	"main": "./out/src/extension",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Example configuration",
			"properties": {
				"kivy.trace.server": {
					"scope": "window",
					"type": "string",
					"enum": [
						"off",
						"messages",
						"verbose"
					],
					"default": "off",
					"description": "Traces the communication between VSCode and the language server."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -w -p ./",
		"test": "mocha -r ts-node/register test/**/*.test.ts"
	},
	"dependencies": {
		"@types/vscode": "^1.46.0",
		"chai": "^4.2.0",
		"mocha": "^8.4.0",
		"ts-node": "^7.0.1",
		"vscode-languageclient": "^5.2.1",
		"vscode-test": "^1.4.0"
	}
}
